<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.mysecretary.viewcontroller.NoteController">
   <children>
      <VBox prefHeight="576.0" prefWidth="793.0" stylesheets="@../design/Dashboard.css">
         <children>
            <Pane fx:id="pnl_note" prefHeight="576.0" prefWidth="793.0" styleClass="pane-right" stylesheets="@../design/Dashboard.css">
               <children>
                  <HBox alignment="CENTER_LEFT" layoutX="45.0" layoutY="100.0" prefHeight="42.0" prefWidth="712.0" spacing="40.0">
                     <children>
                        <Label minWidth="-Infinity" prefWidth="150.0" text="Title" textFill="#e7e5e5">
                           <HBox.margin>
                              <Insets left="30.0" />
                           </HBox.margin>
                        </Label>
                        <Label minWidth="-Infinity" prefWidth="400.0" text="Description" textFill="#e7e5e5">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </Label>
                        <Button fx:id="btn_noteadd" mnemonicParsing="false" onAction="#add_Note" style="-fx-border-color: #f42d76; -fx-border-radius: 20; -fx-background-color: transparent;" stylesheets="@../design/Dashboard.css" text="ADD" textFill="#e7e5e5" />
                     </children>
                  </HBox>
                  <ScrollPane hbarPolicy="NEVER" layoutX="14.0" layoutY="150.0" prefHeight="400.0" prefWidth="746.0" stylesheets="@../design/Dashboard.css">
                     <content>
                        <VBox fx:id="vbox_noteitemslist" prefHeight="400.0" prefWidth="746.0" spacing="5.0" style="-fx-background-color: #38383e;">
                           <padding>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </padding>
                        </VBox>
                     </content>
                  </ScrollPane>
               </children>
            </Pane>
         </children>
      </VBox>
   </children>
</AnchorPane>
