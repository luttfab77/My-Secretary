<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="680.0" prefWidth="980.0" style="-fx-background-color: transparent;" stylesheets="@/at/mysecretary/design/login.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.mysecretary.viewcontroller.login.LoginController">
    <Pane layoutX="2.0" layoutY="140.0" prefHeight="400.0" prefWidth="980.0" style="-fx-background-color: #28272f; -fx-background-radius: 15px;">
        <Label fx:id="lbl_noaccount" layoutX="21.0" layoutY="150.0" text="Don't have an account?" textFill="WHITE">
            <font>
                <Font size="20.0"/>
            </font>
        </Label>
        <Button fx:id="btn_signup" layoutX="21.0" layoutY="200.0" mnemonicParsing="false" onAction="#openSignup" prefHeight="32.0" prefWidth="93.0" styleClass="trans-btn-pink-border" text="Sign Up"/>
        <Label fx:id="lbl_haveaccount" layoutX="759.0" layoutY="150.0" text="Have an account?" textFill="WHITE">
            <font>
                <Font size="20.0"/>
            </font>
        </Label>
        <Button fx:id="btn_signin" layoutX="759.0" layoutY="200.0" mnemonicParsing="false" onAction="#openSignin" prefHeight="32.0" prefWidth="93.0" styleClass="trans-btn-pink-border" text="Sign In"/>
        <VBox id="vbox" fx:id="vbox" layoutX="21.0" layoutY="-49.0" prefHeight="480.0" prefWidth="480.0"
              styleClass="white-pane"/>
    </Pane>
</AnchorPane>
